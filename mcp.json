      "args": ["mcp-server-time", "--local-timezone=America/New_York"]
    },
    "filesystem": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-filesystem",
        "C:\\Users\\Jason\\Desktop"
      ]
    },
    "browser-use": {
      "command": "uv",
      "args": [
        "--directory",
        "C:\\Users\\Jason\\PycharmProjects\\mcp-browser-use",
        "run",
        "src\\mcp_server_browser_use\\server.py"
      ],
      "env":{
        "MCP_API_KEY":"sk-5de1d410783e4cf4a80fabca647a2477",
